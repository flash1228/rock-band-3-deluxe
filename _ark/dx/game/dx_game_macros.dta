#define DX_TRACKSPEED_HANDLER_PENDING
(
   {do
      {set $track_speed_pending_array ()}
      {resize $track_speed_pending_array 0}
      {set $dx_checkedviewtime FALSE}
      {beatmatch foreach_active_player $player
         {if {! $dx_checkedviewtime}
            {unless {== {$player instrument} vocals}
               {switch {$player instrument}
                  (real_keys {set $dx_checkedviewtime TRUE} {set $track_speed_pending_array DX_TRACKSPEED_HANDLER_REAL_KEYS} $track_speed_pending_array)
                  (bass {set $dx_checkedviewtime TRUE} {set $track_speed_pending_array DX_TRACKSPEED_HANDLER_BASS} $track_speed_pending_array)
                  ((drum real_drum) {set $dx_checkedviewtime TRUE} {set $track_speed_pending_array DX_TRACKSPEED_HANDLER_DRUM} $track_speed_pending_array)
                  (guitar {set $dx_checkedviewtime TRUE} {set $track_speed_pending_array DX_TRACKSPEED_HANDLER_GUITAR} $track_speed_pending_array)
                  (real_guitar {set $dx_checkedviewtime TRUE} {set $track_speed_pending_array DX_TRACKSPEED_HANDLER_REAL_GUITAR} $track_speed_pending_array)
                  (real_bass {set $dx_checkedviewtime TRUE} {set $track_speed_pending_array DX_TRACKSPEED_HANDLER_REAL_BASS} $track_speed_pending_array)
                  (keys {set $dx_checkedviewtime TRUE} {set $track_speed_pending_array DX_TRACKSPEED_HANDLER_KEYS} $track_speed_pending_array)
               }
            }
         }
      }
   }
)