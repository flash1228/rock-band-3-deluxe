{push_back $dx_settings_main
   (dxState_ColorSelector
      (view
         os_setguitarcol
         os_setdrumcol
         os_setprotarcol
         os_emissivecolor
      )
      (select
         (os_setguitarcol {dx_state dxState_GuitarColorSelector})
         (os_setdrumcol {dx_state dxState_DrumColorSelector})
